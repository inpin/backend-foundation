{
  "name": "inpin/backend-foundation",
  "description": "Backend (Laravel) foundation base classes",
    "type": "package",
    "license": "MIT",
    "keywords": ["trait", "lumen", "laravel", "eloquent", "request"],
    "authors": [
        {
            "name": "Mohammad Nourinik",
            "email": "enourinick@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
      "laravel/framework": ">=5.6",
      "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6",
        "orchestra/database": "^3.6",
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "~1.0",
        "fzaninotto/faker": "~1.4"
    },
    "autoload": {
        "psr-4": {
            "Inpin\\Foundation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
